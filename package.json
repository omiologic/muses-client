{
  "name": "muses-client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build --dev",
    "dist": "ng build --prod",
    "clean": "rimraf dist/*",
    "e2e": "ng build --no-progress && concurrently \"npm run json-server\" \"ng e2e --port 4200 --proxy-config config/proxy.conf.json\" --kill-others --success first",
    "e2e-no-server": "ng e2e --no-serve --proxy-config config/proxy.conf.json",
    "e2e-report": "open e2e/reports/html/feature_test_report.html",
    "hmr": "ng serve --open --hmr -e=hmr --proxy-config config/proxy.conf.json",
    "json-server": "nodemon --watch ./json-server/ ./json-server/server.js --delay 2",
    "lint": "ng lint --type-check",
    "ng": "ng",
    "start": "ng serve --proxy-config config/proxy.conf.json",
    "test": "cross-env NODE_ENV=test ng test --watch=false",
    "test-browser": "ng test",
    "coverage": "cross-env NODE_ENV=test ng test -cc",
    "coverage-open": "open ./coverage/index.html",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/form": "^6.0.1",
    "@angular-redux/router": "^6.0.1",
    "@angular-redux/store": "^6.0.1",
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "redux-persist": "^4.7.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@angularclass/hmr": "^1.2.2",
    "@types/cucumber": "^2.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~7.0.18",
    "@types/redux-logger": "^3.0.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.0.1",
    "codelyzer": "~3.1.2",
    "concurrently": "^3.4.0",
    "cucumber": "^2.3.1",
    "cucumber-html-reporter": "^2.0.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "json-server": "0.10.0",
    "jsonwebtoken": "^8.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "nodemon": "1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor-cucumber-framework": "^3.1.2",
    "select2": "^4.0.3",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
  "config": {
    "clientPort": "4200",
    "apiPort": "3000",
    "defaultTarget": "http://localhost:",
    "baseURL": "/api/"
  }
}
